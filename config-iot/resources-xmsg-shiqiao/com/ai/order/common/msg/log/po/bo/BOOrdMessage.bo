      </attr>
      <attr floatlength="0" name="REGION_ID" maxlength="6" datatype="String" type="COL" remark="地区">
        <mapingcol datatype="VARCHAR2">REGION_ID</mapingcol>
      </attr>
      <attr floatlength="0" name="MSG_TOPIC" maxlength="100" datatype="String" type="COL" remark="消息主题">
        <mapingcol datatype="VARCHAR2">MSG_TOPIC</mapingcol>
      </attr>
      <attr floatlength="0" name="MSG_SOURCE" maxlength="16" datatype="String" type="COL" remark="消息来源：PRODUCER/CONSUMER">
        <mapingcol datatype="VARCHAR2">MSG_SOURCE</mapingcol>
      </attr>
      <attr floatlength="0" name="MSG_HEADER_MAP" maxlength="1500" datatype="String" type="COL" remark="消息头部信息">
        <mapingcol datatype="VARCHAR2">MSG_HEADER_MAP</mapingcol>
      </attr>
    </attrlist>
    <relationlist />
    <oplist />
  </sysbo>
</sysbolist>
